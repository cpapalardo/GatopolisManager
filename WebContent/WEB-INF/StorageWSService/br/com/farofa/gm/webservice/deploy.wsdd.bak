<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from StorageWSService WSDL service -->

  <service name="StorageWS" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://webservice.gm.farofa.com.br"/>
      <parameter name="wsdlServiceElement" value="StorageWSService"/>
      <parameter name="schemaQualified" value="http://webservice.gm.farofa.com.br"/>
      <parameter name="wsdlServicePort" value="StorageWS"/>
      <parameter name="className" value="br.com.farofa.gm.webservice.StorageWSSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="StorageWS"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="savePhoto" qname="operNS:savePhoto" xmlns:operNS="http://webservice.gm.farofa.com.br" returnQName="retNS:savePhotoReturn" xmlns:retNS="http://webservice.gm.farofa.com.br" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:encodedPhoto" xmlns:pns="http://webservice.gm.farofa.com.br" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:name" xmlns:pns="http://webservice.gm.farofa.com.br" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="savePhoto"/>

  </service>
</deployment>
